<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:width="16dp" android:height="16dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ff9dc8c8" />
            <corners android:topLeftRadius="8dp" android:topRightRadius="8dp" android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp" />
        </shape>
    </item>

    <item android:state_checked="true" android:width="16dp" android:height="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#fffdc23e" />
        </shape>
    </item>
</selector>